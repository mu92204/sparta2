let arr1 = [[1,2],[2,3]]	
let arr2 = [[3,4],[5,6]]

function solution(arr1, arr2) {
  var answer = (arr1, arr2);
  
  for(let i=0; i<arr1.length; i++){
      let sum = (arr1, arr2);
      for(let j=0; i<arr2.length; j++){
        let sum = (arr1, arr2);
      answer.push(sum)
  }}
  return answer;
}
